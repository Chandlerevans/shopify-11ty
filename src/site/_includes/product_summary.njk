<div class="product_summary">
  <a href="/product/{{ item.node.handle }}">{{ item.node.title }}</a> - From {{ item.node.priceRange.minVariantPrice.amount }} ({{ item.node.priceRange.minVariantPrice.currencyCode }})
  <p>{{ item.node.description }}</p>
  <img src="{{item.node.images.edges[0].node.src }}" alt="{{ item.node.images.edges[0].node.alt }}">
</div>
